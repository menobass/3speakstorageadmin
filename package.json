{
  "name": "3speak-storage-admin",
  "version": "1.0.0",
  "description": "CLI tool for managing 3speak storage operations including MongoDB queries, IPFS cleanup, and S3/Wasabi operations",
  "main": "dist/index.js",
  "bin": {
    "3speak-admin": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "watch": "tsc -w",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "3speak",
    "storage",
    "mongodb",
    "ipfs",
    "s3",
    "cli",
    "admin"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.456.0",
    "axios": "^1.13.2",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "inquirer": "^9.2.12",
    "ipfs-http-client": "^60.0.1",
    "mongodb": "^6.3.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
